  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prestart": "sh scripts/check-node.sh",
    "prestart:dev": "sh scripts/check-node.sh",
    "prestart:debug": "sh scripts/check-node.sh",
    "prestart:prod": "sh scripts/check-node.sh",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node prisma/seed.ts",
    "docker:dev": "docker compose up",
    "docker:dev:build": "docker compose up --build",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:prod:build": "docker compose -f docker-compose.prod.yml up -d --build",
    "docker:down": "docker compose down",
    "docker:down:volumes": "docker compose down -v"
  },
